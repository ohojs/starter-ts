{
  "name": "[name]",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "check:type": "tsc --noEmit"
  },
  "keywords": [],
  "homepage": "https://github.com/ohojs/[name]#readme",
  "bugs": {
    "url": "https://github.com/ohojs/[name]/issues"
  },
  "license": "MIT",
  "author": "StEve YoUng <2747745470@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohojs/[name].git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.6",
    "@antfu/ni": "^0.14.0",
    "@babel/types": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.24",
    "bumpp": "^7.1.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "pnpm": "^6.32.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@^6"
}
